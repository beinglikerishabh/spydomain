      a(href='#contactsection') Contact
      a(href='#aboutsection') About
      a(href='localhost:3000') Home
      a.icon(href='javascript:void(0);' style='font-size:15px;' onclick='myFunction()') â˜°
    // Middle Section
    .middle-section
      span.subdomain All Possible Subdomains
      span.inputdomain
        .domain
        h3 
          - for (var i = 0 ; i< domain.length ; i++) {
            p=[i+1]+". "+ domain[i]
          -}
